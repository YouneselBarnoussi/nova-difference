{
    "name": "youbar/nova-difference",
    "description": "A Laravel Nova field to display the difference between to values",
    "keywords": [
        "laravel",
        "nova",
        "difference",
        "field",
        "package"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Youbar\\Difference\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Youbar\\Difference\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
